#include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    cin>>n;
    bool prime=true;
    if(n==1){
        prime=false;
        cout<<prime<<endl;
        return 0;

    }
    
    for(int i=2;i*i<=n;i++){
        if(n%i==0){
            prime=false;
            break;
        }
    }
	
    cout<<prime<<endl;
}
